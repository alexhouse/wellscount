{% extends 'layout.twig' %}

{% block content %}

<h1>Wells Count, A History</h1>
<table class="history table">
    <colgroup>
        <col width="200"/>
        <col width="100"/>
    </colgroup>
    <thead>
        <tr>
            <th>Date</th>
            <th>Count</th>
        </tr>
    </thead>
    <tbody>
        {% for date,count in history %}
            <tr>
                <td>{{ date }}</td>
                <td><strong>{{ count }}</strong></td>
            </tr>
        {% endfor %}
    </tbody>
</table>


{% endblock %}
